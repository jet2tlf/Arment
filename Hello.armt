class Math
    fun Math(num1, num2)
        var this.num1 = num1
        var this.num2 = num2
    end

    fun Sum()
        return this.num1 + this.num2
    end
end

var number1 = input_int("Primeiro Numero: ")
var number2 = input_int("Segundo Numero: ")
var a = Math(number1, number2)
print(a.Sum())